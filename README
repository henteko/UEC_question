sample Twitter.rb
require 'rubygems'
require 'twitter'


Twitter.configure do |config|
    config.consumer_key = "自分の入れてね"
    config.consumer_secret    = "自分の入れてね"
    config.oauth_token        = "取得して入れてね"
    config.oauth_token_secret = "取得して入れてね"
end

Twitter.update(ARGV[0])
